{
  "manifest_version": 2,

  "name": "Memex",
  "description": "This is an implementation of the memex inspired by Vannevar Bush",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icons/icon.png",
    "default_popup": "popup/popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "storage",
    "tabs"
  ]
}
